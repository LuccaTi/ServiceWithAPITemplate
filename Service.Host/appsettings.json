{
  // Service
  "ServiceLogging": {
    "LogDirectory": "logs/Host",
    "LogLevel": "Debug"
  },

  "ServiceConfig": {
    "Interval": 10000
  },

  // API
  "API": {
    "UseAPI": "true",
    "UseSwagger": "true",
    "LogDirectory": "logs/API"
  },
  // Logs e configurações do ASP.NET Core
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
